{
  "$schema": "https://biomejs.dev/schemas/1.8.0/schema.json",
  "javascript": {
    "formatter": {
      "semicolons": "asNeeded"
    }
  },
  "json": {
    "formatter": {
      "trailingCommas": "none"
    }
  },
  "formatter": {
    "formatWithErrors": true,

    "indentStyle": "space"
  },
  "linter": {
    "rules": {
      "suspicious": {
        "noExplicitAny": "off"
      }
    }
  }
}
